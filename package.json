{
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:ssr": "vite-ssr dev",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "lint:markup": "vue-tsc --noEmit",
    "prepare": "cd ../../ && husky install htdocs/frontend/.husky",
    "build:ssr": "vite-ssr build",
    "start": "./src/server.js"
  },
  "dependencies": {
    "@vueuse/core": "^5.3.0",
    "@vueuse/head": "^0.6.0",
    "express": "^4.17.1",
    "install": "^0.13.0",
    "mime": "^2.5.2",
    "node-fetch": "^2.6.1",
    "pascal-case": "^3.1.2",
    "remove": "^0.1.5",
    "rfs": "^9.0.4",
    "storyblok-js-client": "^4.1.3",
    "stream": "^0.0.2",
    "vite-ssr": "^0.12.0",
    "vue": "3",
    "vue-router": "4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@vitejs/plugin-vue": "^1.2.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.2.6",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.13.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.5",
    "postcss-import-ext-glob": "^2.0.1",
    "postcss-nested": "^5.0.5",
    "prettier": "2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.2",
    "vite": "^2.4.0",
    "vite-plugin-svg-icons": "^1.0.1",
    "vite-svg-loader": "^2.1.0",
    "vue-tsc": "^0.0.24"
  }
}
